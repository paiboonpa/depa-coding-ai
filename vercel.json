{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["prompt-master.html"] }
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/prompt-master",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
